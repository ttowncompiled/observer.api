  },
  "homepage": "https://github.com/ttowncompiled/observer.api#readme",
  "devDependencies": {
    "chai": "^3.2.0",
    "clang-format": "^1.0.29",
    "mocha": "^2.2.5",
    "tsd": "^0.6.3",
    "typescript": "^1.6.0-dev.20150812"
  },
  "format": "es6"
